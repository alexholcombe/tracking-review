@article{wolfeGuidedSearchUpdated2021,
  title = {Guided {{Search}} 6.0: {{An}} Updated Model of Visual Search},
  shorttitle = {Guided {{Search}} 6.0},
  author = {Wolfe, Jeremy M.},
